Sub Activity_Create(FirstTime As Boolean)
	
	If FirstTime Then
		tmrTimerCallSub.Initialize
		tmrScreen.Initialize("tmrScreenPower",15000)	'--- init, will be reset later
		#if debug
		tmrSplash.Initialize("tmrSplash",1000)  '--- just a splash screen
		#else
		tmrSplash.Initialize("tmrSplash",3200)  '--- just a splash screen	
		#End If
		tmrFilesCheckChange.Initialize("tmrFilesCheckChange",8000) '--- octo files check
		tmrMain.Initialize("tmrMain",2100) 					       '--- main loop
		tmrSplash.Enabled = True
	End If
	
	GetScreenInfo
	
	Dim pm As B4XPagesManager
	pm.Initialize(Activity)
	
End Sub



