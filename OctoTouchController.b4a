'--- 1 = off, 0 = on
Public Sub Dim_ActionBar(On_off As Int)
	
	Try
			
		Dim p As Phone
		If p.SdkVersion >= 14 And p.SdkVersion < 21 Then
			Dim r As Reflector
			r.Target = Activity
			r.RunMethod2("setSystemUiVisibility", On_off, "java.lang.int") '1 = SYSTEM_UI_FLAG_LOW_PROFILE
		End If
		
	Catch
		Log("DimBottomButtons - " & LastException)
	End Try
	
End Sub
